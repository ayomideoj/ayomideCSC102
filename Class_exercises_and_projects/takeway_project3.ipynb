    "#compound project\n",
    "\n",
    "P = 2000\n",
    "R = 4.5\n",
    "t = 3\n",
    "n = 6\n",
    "\n",
    "A = (P*(1+(R/n))**n*t)\n",
    "print(\"Amount is\",A)\n",
    "\n",
    "CI = A-P\n",
    "print(\"Compund Interest is\",CI)\n",
    "\n"
